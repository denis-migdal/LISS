from browser import window;

LISS = window.LISS

html = LISS.require("./index.html")
css  = LISS.require("./index.css" )

WebC = LISS({"html": html,
             "css" : css})

class WebComponent( self.wrapjs(WebC) ):
    
    def __init__(self, value=42):
        super().__init__()
        self.content.append(value)