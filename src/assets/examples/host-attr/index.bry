from browser import self

WebC = wrapjs(self.LISS({
    "css" : ':host([a="42"]){ background: red; }'
}))

class WebComponent(WebC):
    def __init__(this):
        super().__init__()

        this.host.setAttribute("a", "42")
        attr = this.host.getAttribute("a")
        this.content.replaceChildren(attr)