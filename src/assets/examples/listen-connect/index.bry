from browser import self

WebC = wrapjs(self.LISS())

class WebComponent(WebC):
    def __init__(this):
        super().__init__()

        # simulate some changes
        def simul():
            t = this.host.parentElement;
            this.host.remove();
            t.append(this.host);
        self.setTimeout(simul, 0)

    def connectedCallback(this):
        isConnected = str(this.isConnected).lower()
        this.content.append(f"+{isConnected},")
                            
    def disconnectedCallback(this):
        isConnected = str(this.isConnected).lower()
        this.content.append(f"-{isConnected},")
